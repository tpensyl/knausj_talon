list: user.delimiter_pair
-

# Format:
# SPOKEN_FORM: LEFT_DELIMITER RIGHT_DELIMITER
# Use the literal symbols for delimiters (except for whitespace, which is "space")
#
# Examples:
# round: ( )
# pad: space space

round: ( )
squares: [ ]
angle: < >
curly: { }
twin: "' '"
quad: '" "'
ski: ` `
percentages: % %
#pad: space space
bar: | |

escaped quad: '\\" \\"'
escaped twin:  "\\' \\'"
escaped round: \( \)
escaped box: \[ \]
